{
  "redirects": [
    {
      "source": "/character",
      "destination": "/character.html",
      "statusCode": 301
    },
    {
      "source": "/main",
      "destination": "/index.html",
      "statusCode": 301
    },
    {
      "source": "/:path*",
      "destination": "/index.html",
      "statusCode": 200
    }
  ],
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/character.html", "dest": "/dist/character.html" },
    { "src": "/index.html", "dest": "/dist/index.html" }
  ]
}
